{
    "manifest_version": 3,
    "name": "AirWatch Web Scraper",
    "description": "Web Scraper to pull information from AirWatch",
    "version": "1.0",
    "action": {
      "default_popup": "popup.html",
      "default_icon": "scraper-icon.png"
    },
    "permissions": [
        "activeTab",
        "scripting"
    ]
}

